{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "enabledManagers": [
      "regex"
    ],
    "regexManagers": [
      {
        "fileMatch": [
          "airbyte.yaml"
        ],
        "versioningTemplate": "{{#if versioning}}{{{versioning}}}{{else}}semver{{/if}}"
      }
    ],
    "ignoreUnstable": false
  }
